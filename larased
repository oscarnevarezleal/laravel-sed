#!/usr/bin/env php
<?php
use Laraboot\LarasedApplication;

/**
 * Require the autoloader.
 */
if (file_exists(__DIR__.'/../../autoload.php')) {
    require __DIR__.'/../../autoload.php';
} else {
    require __DIR__.'/vendor/autoload.php';
}

$application = new LarasedApplication();
$application->run();